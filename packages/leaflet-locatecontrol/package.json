{
  "name": "@leaflet-extensions/locatecontrol",
  "version": "0.0.2",
  "description": "A useful control to geolocate the user with many options. Used by osm.org and mapbox among many others.",
  "author": "Nikolay Naumenkov<https://github.com/nikolaynau>",
  "license": "MIT",
  "homepage": "https://github.com/nikolaynau/leaflet-extensions#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nikolaynau/leaflet-extensions.git",
    "directory": "packages/leaflet-locatecontrol"
  },
  "bugs": {
    "url": "https://github.com/nikolaynau/leaflet-extensions/issues"
  },
  "keywords": [
    "leaflet"
  ],
  "exports": {
    ".": {
      "import": "./dist/leaflet-locatecontrol.mjs",
      "require": "./dist/leaflet-locatecontrol.cjs"
    },
    "./*": "./*"
  },
  "main": "./dist/leaflet-locatecontrol.cjs",
  "module": "./dist/leaflet-locatecontrol.mjs",
  "unpkg": "./dist/leaflet-locatecontrol.iife.min.js",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "leaflet.locatecontrol": "^0.79.0"
  }
